class Animal:
    def move(self):
        pass 


class Dog(Animal):
    def move(self):
        print("Running")

class Bird(Animal):
    def move(self):
        print("Flying")

class Fish(Animal):
    def move(self):
        print("Swimming")


# Polymorphism
animals = [Dog(), Bird(), Fish()]

for animal in animals:
    animal.move()
